{
    "name": "addiks/more-php-cs-fixers",
    "license": "MIT",
    "type": "library",
    "description": "Extension for PHP-CS-Fixer with more fixers",

    "autoload": {
        "psr-4": {
            "Addiks\\MorePhpCsFixers\\Tests\\Unit\\": "tests/unit/",
            "Addiks\\MorePhpCsFixers\\": "php/"
        }
    },

    "require": {
        "php": ">=7.1",
        "webmozart/assert": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "infection/infection": "^0.11.0",
        "vimeo/psalm": "^2.0",
        "phpstan/phpstan": "^0.11.7"
    }
}
